{
  "presets": [
    ["env", {
      "targets": {
        "browsers": [
          "> 1%",
          "last 2 versions"
        ]
      }
    }],
    "stage-2",
    "react"
  ],
  "plugins": [
    ["css-modules-transform", {
      "camelCase": true,
      "generateScopedName": "[name]__[local]__[hash:base64:5]"
    }]
  ]
}